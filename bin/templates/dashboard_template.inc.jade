extends ./template.inc.jade
include ./mixin.inc.jade

prepend head
	-var not_blank=bootstrap_req=asp_req=dash_req=true
append asp2
	asp baseURL=request.servervariables("URL")
append aspb1
	asp.
		if session("verified")=false then
			session("status")=0
			baseURL=request.servervariables("URL")
			strURL=mid(baseURL,1,instrrev(baseURL,"/"))
			response.redirect(strURL&"login.asp")
		end if
append content
	.site-wrap
		.container-fluid
			block container
append footer
	//Bootstrap core JavaScript
	//Placed at the end of the document so the pages load faster
	+javascript('js/jquery-1.10.2.min.js')
	+javascript('js/bootstrap.min.js')
	+javascript('js/docs.min.js')
	//IE10 viewport hack for Surface/desktop Windows 8 bug
	+javascript('js/ie10-viewport-bug-workaround.js')